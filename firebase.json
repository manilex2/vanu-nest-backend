{
  "functions": [
    {
      "source": "functions/vanu/dist",
      "codebase": "vanu",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log",
        "*.local"
      ],
      "predeploy": [
        "copy \"functions\\vanu\\package.json\" \"functions\\vanu\\dist\"",
        "npm --prefix \"functions/vanu\" run lint",
        "npm --prefix \"functions/vanu\" run build",
        "copy \"functions\\vanu\\.env\" \"functions\\vanu\\dist\"",
        "copy \"functions\\vanu\\package.json\" \"functions\\vanu\\dist\""
      ],
      "runtime": "nodejs20"
    }
  ]
}
